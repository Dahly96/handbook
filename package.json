{
  "name": "@variant/handbook",
  "version": "1.0.2",
  "description": "Variant h√•ndbok tilgjengelig som markdown eller via terminalen.",
  "main": "npm/data.js",
  "bin": {
    "variant": "npm/handbook.js"
  },
  "files": [
    "npm/",
    "src/*.md"
  ],
  "scripts": {
    "precommit": "pretty-quick --staged",
    "css": "postcss ./docs/styles/style.css -r",
    "build": "node tools/generate.js && npm run css",
    "clean": "rm -rf ./docs/*",
    "start": "serve",
    "watch": "onchange 'src/**/*' 'tools/**/*' -- npm run build",
    "precache": "sw-precache --root=docs --static-file-globs='docs/**/*.{html,json,js,css,svg,jpg,png}'"
  },
  "keywords": [
    "cli",
    "markdown",
    "handbook",
    "variant",
    "open source information",
    "transparent company"
  ],
  "author": "Variant AS",
  "license": "MIT",
  "dependencies": {
    "chalk": "^2.4.1",
    "date-fns": "^2.0.0-alpha.16",
    "markdown-it": "^8.4.2",
    "markdown-it-toc-and-anchor": "^4.1.2",
    "marked": "^0.5.0",
    "marked-terminal": "^3.1.1",
    "prompt-list": "^3.2.0",
    "glob-promise": "^3.4.0",
    "glob": "^7.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.3",
    "husky": "^0.14.3",
    "onchange": "^4.1.0",
    "postcss": "^7.0.2",
    "postcss-clean": "^1.1.0",
    "postcss-cli": "^6.0.0",
    "postcss-custom-properties": "^7.0.0",
    "prettier": "1.14.2",
    "pretty-quick": "^1.6.0",
    "recursive-copy": "^2.0.9",
    "serve": "^10.0.0",
    "sw-precache": "^5.2.1"
  }
}
